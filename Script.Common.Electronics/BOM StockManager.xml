          <On Select="TPartKeeprPrefix.Femto">
            <Execute Statement="Result:= Result + cFemto"/>
          </On>
          <On Select="TPartKeeprPrefix.Atto">
            <Execute Statement="Result:= Result + cAtto"/>
          </On>
          <On Select="TPartKeeprPrefix.Zepto">
